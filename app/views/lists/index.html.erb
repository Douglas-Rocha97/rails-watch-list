<div class="container">
  <h1>Save any kind of movies</h1>
  <div class="d-flex justify-content-between">
    <h2>My Lists</h2>
    <%= link_to "Create a movie list", new_list_path ,class: "btn btn-primary" %>
  </div>
  <div class="d-flex">
    <% @lists.each do |list| %>
      <%= link_to list_path(list) do%>
        <p class="ms-3"><%= list.name %></p>
      <% end %>
    <% end %>
  </div>
</div>
